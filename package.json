{
  "name": "mh-search-assist",
  "version": "0.0.1",
  "description": "A productivity extension for MH",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "webpack",
    "pack": "web-ext build --source-dir ./dist/",
    "start:firefox": "web-ext run --source-dir ./dist/",
    "publish:amo": "web-ext-submit --source-dir ./dist/ --api-key=$AMO_API_KEY --api-secret=$AMO_API_SECRET --id=$AMO_EXTENSION_ID",
    "publish:cws": "chrome-webstore-upload upload --source ./dist/ --extension-id $CWS_EXTENSION_ID --client-id $CWS_CLIENT_ID --refresh-token $CWS_REFRESH_TOKEN && chrome-webstore-upload publish --extension-id $CWS_EXTENSION_ID --client-id $CWS_CLIENT_ID --refresh-token $CWS_REFRESH_TOKEN",
    "test": "mocha 'src/**/*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hazzik/mh-search-assist.git"
  },
  "author": "",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/hazzik/mh-search-assist/issues"
  },
  "homepage": "https://github.com/hazzik/mh-search-assist#readme",
  "devDependencies": {
    "chrome-webstore-upload": "^1.0.0",
    "copy-webpack-plugin": "^10.1.0",
    "mocha": "^9.1.3",
    "web-ext": "^6.6.0",
    "web-ext-submit": "^6.6.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "debounce": "^1.2.1"
  }
}
